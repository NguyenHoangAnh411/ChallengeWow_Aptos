# Private key of the account that will deploy contracts
# WARNING: Never commit your private key to version control!
PRIVATE_KEY=your_private_key_here

# Olym3 Testnet API key (if available)
OLYM3_API_KEY=your_olym3_api_key_here

# Enable gas reporting
REPORT_GAS=true

# RPC URLs
OLYM3_RPC_URL=https://rpc.olym3.xyz
OLYM3_EXPLORER_URL=https://explorer.olym3.xyz

# Contract addresses (will be filled after deployment)
GAME_CONTRACT_ADDRESS=
REWARD_CONTRACT_ADDRESS=
ZK_PROOF_CONTRACT_ADDRESS= 